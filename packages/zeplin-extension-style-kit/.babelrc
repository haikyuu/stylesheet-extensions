{
  "presets": [
    ["@babel/preset-env", {
        "targets": {
          "browsers": ["chrome >= 45", "safari >= 9.1", "firefox >= 45"]
        }
    }]
  ],
  "env": {
    "development": {
      "plugins": [
        "@babel/transform-modules-commonjs"
      ]
    },
    "test": {
      "plugins": [
        "@babel/transform-modules-commonjs",
        ["module-resolver", {
          "root": ["./"],
          "alias": {
            "@root": "./",
            "@testHelpers": "./tests/helpers"
          }
        }]
      ]
    }
  }
}
